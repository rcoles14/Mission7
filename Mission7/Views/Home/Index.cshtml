@{
    ViewData["Title"] = "BookList";
}
@model BooksViewModel



    <h1>Books in List</h1><br />
    @foreach (Book b in Model.Books)
    {
        <partial name="BookSummary" model="b"/>
    }

<div page-model="@Model.Pageinfo" page-classes-enabled="true" page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" class="btn-group m-1"></div>
